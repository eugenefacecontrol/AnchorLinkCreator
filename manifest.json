{
    "name": "Anchor Link Creator",
    "description": "This extension will help to create Anchor Links",
    "version": "1.0",
    "background": {
        "persistent": false,
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Anchor Link Creator"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "64": "icon.png"
    },
    "manifest_version": 2
}